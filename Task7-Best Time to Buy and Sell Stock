class Solution {
    public int maxProfit(int[] prices) {
        int minp = prices[0];
        int maxp=0;
        for(int i=0;i<prices.length;i++)
        {
            if(prices[i]<minp)
            {
                minp = prices[i];
            }
            else
            {
                int profit = prices[i]-minp;
                maxp=Math.max(maxp,profit);
            }
        }

        return maxp;
        
    }
}
