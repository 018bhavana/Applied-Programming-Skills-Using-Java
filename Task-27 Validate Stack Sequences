class Solution {
    public boolean validateStackSequences(int[] pushed, int[] popped) {
        int i = 0;
        java.util.Stack<Integer> stack = new java.util.Stack<>();
        for (int x : pushed) {
            stack.push(x);
            while (!stack.isEmpty() && i < popped.length && stack.peek() == popped[i]) {
                stack.pop();
                i++;
            }
        }
        return stack.isEmpty();
    }
}
